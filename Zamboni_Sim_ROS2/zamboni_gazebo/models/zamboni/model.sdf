<?xml version="1.0"?>
<sdf version='1.7'>
  <model name="zamboni">
    <pose>0 0 3 0 0 0</pose>

    <link name='base_link' />

    <joint name='chassis_link_joint' type='fixed'>
      <pose>0.0 0.0 0.0 0 0 0</pose>
      <parent>base_link</parent>
      <child>chassis_link</child>
    </joint>

    <joint name="realsense_joint" type="fixed">
      <parent>base_link</parent>
      <child>sensor_link</child>
      <pose>0 0 0 0 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>

    <link name="sensor_link">
      <pose relative_to='base_link'>0 2.34 0.72 0 0 0</pose>
      <inertial>
        <mass>0.1</mass>
      </inertial>

      <visual name="camera_visual">
        <pose relative_to='base_link'>0 2.34 0.72 0 0 0</pose>
        <geometry>
          <box>
            <size>0.015 0.08 0.022</size>
          </box>
        </geometry>
        <material>
          <ambient>0 0 0 1.0</ambient>
          <diffuse>0 0 0 1.0</diffuse>
          <specular>0.0 0.0 0.0 1.0</specular>
          <emissive>0.0 0.0 0.0 1.0</emissive>
        </material>
      </visual>

      <sensor name="depth_camera" type="depth">
        <pose relative_to='base_link'>0 2.34 0.72 0 0 1.5708</pose>
        <always_on>1</always_on>
        <update_rate>15</update_rate>
        <camera name="depth_camera">
          <horizontal_fov>1.11701</horizontal_fov><!--69.4 deg = 1.211258501 rad -->
          <aspect_ratio>0.715585</aspect_ratio><!-- 16:9-->
          <image>
              <width>640</width>
              <height>480</height>
              <format>B8G8R8</format>
          </image>
          <clip>
              <near>0.1</near>
              <far>10</far>
          </clip>
          <noise>
              <type>gaussian</type>
              <mean>0.0</mean>
              <stddev>0.007</stddev>
          </noise>
        </camera>
        <plugin name="camera_plugin" filename="libgazebo_ros_camera.so">
          <camera_name>depth_camera</camera_name>
          <frame_name>realsense_link</frame_name>
          <hack_baseline>0</hack_baseline>
          <min_depth>0.001</min_depth>
        </plugin>
      </sensor>
    </link>

    <link name='chassis_link'>
      <inertial>
        <pose>1e-06 0 0 0 0 0</pose>
        <mass>1580</mass>
        <inertia>
          <ixx>1997.86</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>442.769</iyy>
          <iyz>62.5206</iyz>
          <izz>2238.05</izz>
        </inertia>
      </inertial>
      <collision name='chassis_link_fixed_joint_lump__chassis_link_collision'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>0.001 0.001 0.001</scale>
            <uri>model://zamboni/meshes/zamboni2.stl</uri>
          </mesh>
        </geometry>
        <surface>
          <contact>
            <ode />
          </contact>
          <friction>
            <ode>
              <mu>0.2</mu>
              <mu2>0.2</mu2>
            </ode>
          </friction>
        </surface>
      </collision>

      <visual name='chassis_link_fixed_joint_lump__chassis_link_visual'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>0.001 0.001 0.001</scale>
            <uri>model://zamboni/meshes/zamboni2.stl</uri>
          </mesh>
        </geometry>
      </visual>
      <self_collide>1</self_collide>
    </link>

    <joint name='bl_axle' type='revolute'>
      <pose relative_to='chassis_link'>-0.69 0 0 0 -0 0</pose>
      <parent>chassis_link</parent>
      <child>bl_1</child>
      <axis>
        <xyz>-1 0 0</xyz>
        <dynamics>
          <friction>12.031606150200002</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name='br_axle' type='revolute'>
      <pose relative_to='chassis_link'>0.69 0 0 0 -0 0</pose>
      <parent>chassis_link</parent>
      <child>br_1</child>
      <axis>
        <xyz>-1 0 0</xyz>
        <dynamics>
          <friction>12.031606150200002</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name='fl_axle' type='revolute'>
      <pose relative_to='l_steer_1'>-0.05 0 0 0 -0 0</pose>
      <parent>l_steer_1</parent>
      <child>fl_1</child>
      <axis>
        <xyz>-1 0 0</xyz>
        <dynamics>
          <friction>12.031606150200002</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name='fr_axle' type='revolute'>
      <pose relative_to='r_steer_1'>0.05 0 0 0 -0 0</pose>
      <parent>r_steer_1</parent>
      <child>fr_1</child>
      <axis>
        <xyz>-1 0 0</xyz>
        <dynamics>
          <friction>12.031606150200002</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name='l_steer' type='revolute'>
      <pose relative_to='chassis_link'>-0.69 1.70 0 0 -0 0</pose>
      <parent>chassis_link</parent>
      <child>l_steer_1</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-0.95</lower>
          <upper>0.95</upper>
        </limit>
        <dynamics>
          <friction>18.0474092253</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name='r_steer' type='revolute'>
      <pose relative_to='chassis_link'>0.69 1.70 0 0 -0 0</pose>
      <parent>chassis_link</parent>
      <child>r_steer_1</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-0.95</lower>
          <upper>0.95</upper>
        </limit>
        <dynamics>
          <damping>0.01</damping>
          <friction>18.0474092253</friction>
        </dynamics>
      </axis>
    </joint>

    <link name='bl_1'>
      <pose relative_to='bl_axle'>0 0 0 0 0 0</pose>
      <inertial>
        <pose>-0.0377 0 -0 0 -0 0</pose>
        <mass>24.5</mass>
        <inertia>
          <ixx>1.84514</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1.04961</iyy>
          <iyz>0</iyz>
          <izz>1.04961</izz>
        </inertia>
      </inertial>
      <collision name='bl_1_collision'>
        <pose relative_to='bl_axle'>0.05 0 0 0 1.5708 0</pose>
        <geometry>
          <cylinder>
            <radius>.297</radius>
            <length>.22</length>
          </cylinder>
        </geometry>
        <surface>
          <contact>
            <ode />
          </contact>
          <friction>
            <ode>
              <mu>1.7</mu>
              <mu2>1.7</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name='bl_1_visual'>
        <pose>0.875 1.0136 0.15 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>0.001 0.001 0.001</scale>
            <uri>model://zamboni/meshes/bl_1.stl</uri>
          </mesh>
        </geometry>
      </visual>
      <self_collide>1</self_collide>
    </link>

    <link name='br_1'>
      <pose relative_to='br_axle'>0 0 0 0 -0 0</pose>
      <inertial>
        <pose>-0.0377 0 -0 0 -0 0</pose>
        <mass>24.5</mass>
        <inertia>
          <ixx>1.84514</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1.04961</iyy>
          <iyz>0</iyz>
          <izz>1.04961</izz>
        </inertia>
      </inertial>
      <collision name='br_1_collision'>
        <pose relative_to='br_axle'>-0.05 0 0 0 1.5708 0</pose>
        <geometry>
          <cylinder>
            <radius>.297</radius>
            <length>.23</length>
          </cylinder>
        </geometry>
        <surface>
          <contact>
            <ode />
          </contact>
          <friction>
            <ode>
              <mu>1.7</mu>
              <mu2>1.7</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name='br_1_visual'>
        <pose>-0.875 1.0136 0.15 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>0.001 0.001 0.001</scale>
            <uri>model://zamboni/meshes/br_1.stl</uri>
          </mesh>
        </geometry>
      </visual>
      <self_collide>1</self_collide>
    </link>

    <link name='fl_1'>
      <pose relative_to='fl_axle'>0 0 0 0 -0 0</pose>
      <inertial>
        <pose>-0.0377 0 -0 0 -0 0</pose>
        <mass>24.5</mass>
        <inertia>
          <ixx>1.84514</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1.04961</iyy>
          <iyz>0</iyz>
          <izz>1.04961</izz>
        </inertia>
      </inertial>
      <collision name='fl_1_collision'>
        <pose>0.7875 -1.3657 0.15 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>0.0009 0.0009 0.0009</scale>
            <uri>model://zamboni/meshes/fl_1.stl</uri>
          </mesh>
        </geometry>
        <surface>
          <contact>
            <ode />
          </contact>
          <friction>
            <ode>
              <mu>1.7</mu>
              <mu2>1.7</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name='fl_1_visual'>
        <pose>0.7875 -1.3657 0.15 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>0.0009 0.0009 0.0009</scale>
            <uri>model://zamboni/meshes/fl_1.stl</uri>
          </mesh>
        </geometry>
      </visual>
      <self_collide>1</self_collide>
    </link>

    <link name='fr_1'>
      <pose relative_to='fr_axle'>0 0 0 0 -0 0</pose>
      <inertial>
        <pose>-0.0377 0 -0 0 -0 0</pose>
        <mass>24.5</mass>
        <inertia>
          <ixx>1.84514</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1.04961</iyy>
          <iyz>0</iyz>
          <izz>1.04961</izz>
        </inertia>
      </inertial>
      <collision name='fr_1_collision'>
        <pose>-0.7875 -1.3657 0.15 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>0.0009 0.0009 0.0009</scale>
            <uri>model://zamboni/meshes/fr_1.stl</uri>
          </mesh>
        </geometry>
        <surface>
          <contact>
            <ode />
          </contact>
          <friction>
            <ode>
              <mu>1.7</mu>
              <mu2>1.7</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name='fr_1_visual'>
        <!-- <pose>-0.875 -1.5174 0 0 -0 0</pose> -->
        <pose>-0.7875 -1.3657 0.15 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>0.0009 0.0009 0.0009</scale>
            <uri>model://zamboni/meshes/fr_1.stl</uri>
          </mesh>
        </geometry>
      </visual>
      <self_collide>1</self_collide>
    </link>

    <link name='l_steer_1'>
      <pose relative_to='l_steer'>0 0 0 0 -0 0</pose>
      <inertial>
        <pose>0 0 0 0 -0 0</pose>
        <mass>1</mass>
        <inertia>
          <ixx>1</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1</iyy>
          <iyz>0</iyz>
          <izz>1</izz>
        </inertia>
      </inertial>
      <self_collide>1</self_collide>
    </link>

    <link name='r_steer_1'>
      <pose relative_to='r_steer'>0 0 0 0 -0 0</pose>
      <inertial>
        <pose>0 0 0 0 -0 0</pose>
        <mass>1</mass>
        <inertia>
          <ixx>1</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1</iyy>
          <iyz>0</iyz>
          <izz>1</izz>
        </inertia>
      </inertial>
      <!-- <visual name="camera_visual">
        <geometry>
          <box>
            <size>0.015 0.08 0.022</size>
          </box>
        </geometry>
        <material>
          <ambient>0 0 0 1.0</ambient>
          <diffuse>0 0 0 1.0</diffuse>
          <specular>0.0 0.0 0.0 1.0</specular>
          <emissive>0.0 0.0 0.0 1.0</emissive>
        </material>
      </visual> -->

      <self_collide>1</self_collide>
    </link>

    <link name="steering_wheel" />

    <joint name="steering_joint" type="revolute">
      <pose>-0.002 0 0 0 0 0</pose>
      <parent>chassis_link</parent>
      <child>steering_wheel</child>
      <axis>
        <xyz>-0.964118 0.000768 0.265556</xyz>
        <limit>
          <lower>-100</lower>
          <upper>100</upper>
        </limit>
        <dynamics>
          <damping>1.0</damping>
        </dynamics>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <cfm_damping>1</cfm_damping>
        </ode>
      </physics>
    </joint>

    <plugin name='ackermann_drive' filename='libgazebo_ros_ackermann_drive.so'>
      <ros>
        <namespace>zamboni</namespace>
        <remapping>cmd_vel:=cmd_vel</remapping>
        <remapping>odom:=odom</remapping>
        <remapping>distance:=distance</remapping>
      </ros>

      <update_rate>100.0</update_rate>

      <!-- wheels -->
      <front_left_joint>fl_axle</front_left_joint>
      <front_right_joint>fr_axle</front_right_joint>
      <rear_left_joint>bl_axle</rear_left_joint>
      <rear_right_joint>br_axle</rear_right_joint>
      <left_steering_joint>l_steer</left_steering_joint>
      <right_steering_joint>r_steer</right_steering_joint>
      <steering_wheel_joint>steering_joint</steering_wheel_joint>

      <!-- Max absolute steer angle for tyre in radians-->
      <!-- Any cmd_vel angular z greater than this would be capped -->
      <max_steer>0.85</max_steer>

      <!-- Max absolute steering angle of steering wheel -->
      <!-- Max absolute steering angle of steering wheel -->
      <max_steering_angle>7.85</max_steering_angle>

      <!-- Max absolute linear speed in m/s -->
      <max_speed>20</max_speed>

      <!-- PID tuning -->
      <left_steering_pid_gain>4000 0 1</left_steering_pid_gain>
      <left_steering_i_range>0 0</left_steering_i_range>
      <right_steering_pid_gain>4000 0 1</right_steering_pid_gain>
      <right_steering_i_range>0 0</right_steering_i_range>
      <linear_velocity_pid_gain>4000 0 1</linear_velocity_pid_gain>
      <linear_velocity_i_range>0 0</linear_velocity_i_range>

      <!-- output -->
      <publish_odom>true</publish_odom>
      <publish_odom_tf>true</publish_odom_tf>
      <publish_wheel_tf>true</publish_wheel_tf>
      <publish_distance>true</publish_distance>

      <odometry_frame>odom</odometry_frame>
      <robot_base_frame>base_link</robot_base_frame>

    </plugin>
  </model>
</sdf>
